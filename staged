[32m+[m[32mfunction timeDiff(timezone: string) {[m
[32m+[m[32m  const localTime = DateTime.local().offset;[m
[32m+[m
[32m+[m[32m  const diffTimezone = DateTime.local().setZone(timezone).offset;[m
[32m+[m
[32m+[m[32m  const diffInHours = (localTime - diffTimezone) / 60;[m
[32m+[m
[32m+[m[32m  if (diffInHours > 0) {[m
[32m+[m[32m    return `${diffInHours} hours behind`;[m
[32m+[m[32m  } else if (diffInHours === 0) {[m
[32m+[m[32m    return `same as local time`;[m
[32m+[m[32m  } else {[m
[32m+[m[32m    return `${-1 * diffInHours} hours ahead`;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m[32mexport const WorldTimeCard: React.FC<WorldTimeCardProps> = ({[m

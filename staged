[32m+[m[32m  WorldTimeAction,[m
[32m+[m[32m} from '../hooks/useLocalStorage';[m
[32m+[m[32mexport const WorldTimeContext = React.createContext<{[m
[32m+[m[32m  userWorldTimeList: ListArrayType;[m
[32m+[m[32m  dispatch: React.Dispatch<WorldTimeAction>;[m
[32m+[m[32m}>({ userWorldTimeList: [], dispatch: () => {} });[m
[32m+[m
[32m+[m[32mexport const WorldTimeContextProvider: React.FC = ({ children }) => {[m
[32m+[m[32m  const { worldTimeList, dispatch } = useLocalStorage();[m
[32m+[m[32m  return ([m
[32m+[m[32m    <WorldTimeContext.Provider[m
[32m+[m[32m      value={{[m
[32m+[m[32m        userWorldTimeList: worldTimeList,[m
[32m+[m[32m        dispatch: dispatch,[m
[32m+[m[32m      }}>[m
[32m+[m[32m      {children}[m
[32m+[m[32m    </WorldTimeContext.Provider>[m
[32m+[m[32m  );[m
[32m+[m[32m};[m
[1mdiff --git a/src/hooks/useLocalStorage.tsx b/src/hooks/useLocalStorage.tsx[m
[1mnew file mode 100644[m
[1mindex 0000000..069fb54[m
[1m--- /dev/null[m
[1m+++ b/src/hooks/useLocalStorage.tsx[m
[36m@@ -0,0 +1,68 @@[m
[32m+[m[32mimport { useReducer, useEffect } from 'react';[m
[32m+[m[32mimport { Plugins } from '@capacitor/core';[m
[32m+[m
[32m+[m[32mconst { Storage } = Plugins;[m
[32m+[m
[32m+[m[32mexport enum TYPES {[m
[32m+[m[32m  DELETE = 'delete',[m
[32m+[m[32m  ADD = 'add',[m
[32m+[m[32m}[m
[32m+[m[32mexport type ListArrayType = Array<{[m
[32m+[m[32m  id?: string;[m
[32m+[m[32m  city?: string;[m
[32m+[m[32m  timezone?: string;[m
[32m+[m[32m}>;[m
[32m+[m
[32m+[m[32mexport type WorldTimeAction = {[m
[32m+[m[32m  type: TYPES.ADD | TYPES.DELETE;[m
[32m+[m[32m  payload: { id: string; city?: string; timezone?: string };[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mfunction reducer(state: ListArrayType, action: WorldTimeAction) {[m
[32m+[m[32m  switch (action.type) {[m
[32m+[m[32m    case TYPES.ADD:[m
[32m+[m[32m      return [[m
[32m+[m[32m        ...state,[m
[32m+[m[32m        {[m
[32m+[m[32m          id: action.payload.id,[m
[32m+[m[32m          city: action.payload.city,[m
[32m+[m[32m          timezone: action.payload.timezone,[m
[32m+[m[32m        },[m
[32m+[m[32m      ];[m
[32m+[m[32m    case TYPES.DELETE:[m
[32m+[m[32m      return state.filter((prevState) => prevState.id === action.payload.id);[m
[32m+[m[32m    default:[m
[32m+[m[32m      return state;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m[32mconst initialState = [{ id: '1', city: 'local', timezone: 'local' }];[m
[32m+[m[32mconst KEY = 'WeatherApp-worldList';[m
[32m+[m[32mexport const useLocalStorage = () => {[m
[32m+[m[32m  const [worldTimeList, dispatch] = useReducer(reducer, []);[m
[32m+[m[32m  useEffect(() => {[m
[32m+[m[32m    const addList = async () => {[m
[32m+[m[32m      const { value } = await Storage.get({ key: KEY });[m
[32m+[m
[32m+[m[32m      if (value != null) {[m
[32m+[m[32m        const InitialValues = JSON.parse(value);[m
[32m+[m[32m        if ([m
[32m+[m[32m          InitialValues.length > worldTimeList.length &&[m
[32m+[m[32m          worldTimeList.length === 0[m
[32m+[m[32m        ) {[m
[32m+[m[32m          InitialValues.forEach((InitialValue: any) => {[m
[32m+[m[32m            dispatch({ type: TYPES.ADD, payload: InitialValue });[m
[32m+[m[32m          });[m
[32m+[m[32m        } else if (InitialValues.length === worldTimeList.length) {[m
[32m+[m[32m          return null;[m
[32m+[m[32m        } else if (InitialValues.length < worldTimeList.length) {[m
[32m+[m[32m          await Storage.set({ key: KEY, value: JSON.stringify(worldTimeList) });[m
[32m+[m[32m        }[m
[32m+[m[32m      } else {[m
[32m+[m[32m        await Storage.set({ key: KEY, value: JSON.stringify(initialState) });[m
[32m+[m[32m      }[m
[32m+[m[32m    };[m
[32m+[m[32m    addList();[m
[32m+[m[32m  }, [worldTimeList]);[m
[32m+[m
[32m+[m[32m  return { worldTimeList, dispatch };[m
[32m+[m[32m};[m

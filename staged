[32m+[m[32m  useEffect(() => {[m
[32m+[m[32m    let isMounted = true;[m
[32m+[m[32m    if (isMounted) {[m
[32m+[m[32m      setInterval(() => setTime(DateTime.local().toFormat('HH:mm')), 1000);[m
[32m+[m[32m      setDate(DateTime.local().toLocaleString(DateTime.DATE_MED_WITH_WEEKDAY));[m
[32m+[m[32m    }[m
[32m+[m[32m    return () => {[m
[32m+[m[32m      clearInterval();[m
[32m+[m[32m      isMounted = false;[m
[32m+[m[32m    };[m
[32m+[m[32m  }, [time]);[m
[32m+[m
   return ([m
     <IonCard>[m
